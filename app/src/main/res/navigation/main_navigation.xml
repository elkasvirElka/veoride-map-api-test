<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/map_fragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/map_fragment"
        android:name="com.elviraminnullina.map_api.ui.map.MapFragment">
        <action
            android:id="@+id/map_fragment_to_travel_info_fragment"
            app:destination="@+id/travel_info_fragment" />
    </fragment>


    <fragment
        android:id="@+id/travel_info_fragment"
        android:name="com.elviraminnullina.map_api.ui.travel_info.TravelInformationFragment" />
</navigation>